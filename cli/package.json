{
  "name": "create-nestjs-auth",
  "version": "1.0.0",
  "description": "Instant NestJS authentication setup with JWT, OAuth, RBAC, and security best practices",
  "author": "Sabin Shrestha <sabin.shrestha.er@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "authentication",
    "cli",
    "boilerplate",
    "jwt",
    "oauth",
    "typescript",
    "security",
    "rbac",
    "prisma",
    "create-nestjs-auth"
  ],
  "bin": {
    "create-nestjs-auth": "./bin/create-nestjs-auth.js",
    "cna": "./bin/create-nestjs-auth.js"
  },
  "files": [
    "dist",
    "templates",
    "bin"
  ],
  "preferGlobal": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "tsc && npm run copy-templates",
    "copy-templates": "node scripts/copy-templates.js",
    "dev": "npm run build && node bin/create-nestjs-auth.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "ejs": "^3.1.10",
    "enquirer": "^2.4.1",
    "execa": "^5.1.1",
    "fs-extra": "^11.2.0",
    "ora": "^5.4.1",
    "validate-npm-package-name": "^5.0.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.7",
    "@types/validate-npm-package-name": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/masabinhok/production-ready-nestjs-auth.git",
    "directory": "cli"
  },
  "bugs": {
    "url": "https://github.com/masabinhok/production-ready-nestjs-auth/issues"
  },
  "homepage": "https://github.com/masabinhok/production-ready-nestjs-auth#readme"
}
